package main.java.hashtable;

/**
 * algo is do the sum of all element and hash each sum now for each point sum -
 * given sum is already seen then subraray exists
 * 
 * @author rdixi3
 *
 */
public class SubArrayWithGivenSum {

	public static boolean subArrayExists(int[] elem, int sum) {
		for (int i = 0; i < elem.length; i++) {
			int indexSum = indexSum + elem[i];
		}
	}
}
